{
  "name": "@masscholar/web",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "dev:app": "pnpm --filter @masscholar/app run dev",
    "dev:app:renderer": "pnpm --filter @masscholar/app run dev:renderer",
    "dev:storybook": "pnpm --filter @masscholar/ui run storybook",
    "dev:editor": "pnpm --filter @masscholar/editor run dev",
    "build:ui": "pnpm --filter @masscholar/ui run build",
    "build:crates:ms-core": "pnpm --filter ms-core run build",
    "build:crates:ms-database": "pnpm --filter ms-database run build",
    "build:crates:ms-simulation": "pnpm --filter ms-simulation run build",
    "build:crates:ms-statistics": "pnpm --filter ms-statistics run build",
    "test:datasource": "",
    "test:editor": "",
    "test:ui": "",
    "test:crates:ms-core": "pnpm --filter ms-core run test",
    "test:crates:ms-database": "pnpm --filter ms-database run test",
    "test:crates:ms-simulation": "pnpm --filter ms-simulation run test",
    "test:crates:ms-statistics": "pnpm --filter ms-statistics run test",
    "test:all": "",
    "generate:preload": "pnpm --filter @masscholar/app run generate:preload",
    "add:dep:app": "pnpm --filter @masscholar/app add",
    "add:dep:ui": "pnpm --filter @masscholar/ui add",
    "add:dep:chart": "pnpm --filter @masscholar/chart add",
    "add:dep:editor": "pnpm --filter @masscholar/editor add",
    "add:dep:datasource": "pnpm --filter @masscholar/datasource add",
    "add:ui:component": "pnpm --filter @masscholar/ui add:component",
    "cleandep": "rm -rf node_modules target pnpm-lock.yaml && pnpm -r exec rm -rf node_modules"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "electron",
      "esbuild",
      "sharp",
      "skia-canvas",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "author": "Kaifuny <kaifuny.github@gmail.com>",
  "license": "AGPL-3.0-only"
}