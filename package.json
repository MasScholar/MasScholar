{
  "name": "@masscholar/web",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  },
  "scripts": {
    "dev:web": "pnpm --filter @masscholar/web run dev",
    "dev:app": "pnpm --filter @masscholar/app run dev",
    "dev:storybook": "pnpm --filter @masscholar/ui run storybook",
    "build:ui": "pnpm --filter @masscholar/ui run build",
    "build:web": "pnpm --filter @masscholar/web run build",
    "test:all": "",
    "add:dep:app": "pnpm --filter @masscholar/app add",
    "add:dep:ui": "pnpm --filter @masscholar/ui add",
    "add:dep:chart": "pnpm --filter @masscholar/chart add",
    "add:dep:agent": "pnpm --filter @masscholar/agent add",
    "add:dep:core": "pnpm --filter @masscholar/core add",
    "add:dep:datasource": "pnpm --filter @masscholar/datasource add",
    "add:ui:component": "pnpm --filter @masscholar/ui add:component",
    "cleandep": "rm -rf node_modules target pnpm-lock.yaml && pnpm -r exec rm -rf node_modules"
  },
