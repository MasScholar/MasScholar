{
  "i18n-ally.localesPaths": [
    "packages/ms-i18n/src/locales"
  ],
  "i18n-ally.keystyle": "nested",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "tsconfig.json": "tsconfig.*.json, env.d.ts",
    "vite.config.*": "jsconfig*, vitest.config.*, cypress.config.*, playwright.config.*",
    "package.json": "package-lock.json, pnpm*, .yarnrc*, yarn*, .eslint*, eslint*, .oxlint*, oxlint*, .prettier*, prettier*, .editorconfig"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.rust-analyzer": true,
    "**/.turbo": true,
    "**/tsconfig.node.tsbuildinfo": true,
    "**/tsconfig.tsbuildinfo": true,
    "**/.nuxt": true,
    "**/.data": true,
    "**/target": true,
    "**/.temp": true,
    "**/.cache": true,
    "**/dist": true
  },
  "catppuccin-icons.associations.folders": {
    "electron": "folder_client",
    "stores": "folder_redux",
    "lib": "folder_utils",
    "schema": "folder_config",
    "storage": "folder_database",
    "checkpoint": "folder_content",
    "runtime": "folder_devcontainer",
    "scheduler": "folder_composables",
    "vendor": "folder_git",
  },
  "tailwindCSS.experimental.classRegex": [
    "class:\\s*?[\"'`]([^\"'`]*).*?,"
  ],
  "gitConfigUser.selectedProfile": "Kaifuny"
}